{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "no-var-requires": false,
    "interface-name": [
      false
    ],
    "variable-name": [
      false
    ],
    "max-line-length": [
      false
    ],
    "object-literal-sort-keys": false,
    "forin": false,
    "arrow-parens": false,
    "no-unused-variable": [
      false
    ],
    "quotemark": [true, "single"],
    "ordered-imports": [
      false
    ],
    "only-arrow-functions": [
      false
    ],
    "no-string-literal": false,
    "no-console": [
      false
    ]
  }
}